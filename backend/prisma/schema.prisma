generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model artists {
  id                     Int                      @id
  name                   String?
  real_name              String?
  profile                String?
  data_quality           String?
  full_data              Json?
  artist_aliases         artist_aliases[]
  artist_groups          artist_groups[]
  artist_members         artist_members[]
  artist_name_variations artist_name_variations[]
  artist_urls            artist_urls[]
}

model artist_urls {
  artist_id Int?
  url       String?
  id        Int      @id @default(autoincrement())
  artists   artists? @relation(fields: [artist_id], references: [id])
}

model artist_aliases {
  artist_id  Int?
  alias_name String?
  id         Int      @id @default(autoincrement())
  artists    artists? @relation(fields: [artist_id], references: [id])
}

model artist_name_variations {
  artist_id Int?
  variation String?
  id        Int      @id @default(autoincrement())
  artists   artists? @relation(fields: [artist_id], references: [id])
}

model artist_members {
  artist_id   Int?
  member_name String?
  id          Int      @id @default(autoincrement())
  artists     artists? @relation(fields: [artist_id], references: [id])
}

model artist_groups {
  artist_id  Int?
  group_name String?
  id         Int      @id @default(autoincrement())
  artists    artists? @relation(fields: [artist_id], references: [id])
}
